package day38;

import java.util.LinkedList;
import java.util.Queue;

public class QueueEx {

	public static void main(String[] args) {
		Queue<String>q=new LinkedList<>();
		//1. add(e)
		q.add("apple");	
		q.add("banana");	
		q.add("cherry");	
		q.add("tomato");
		System.out.println(q);
		
		//2. element
		System.out.println("element:"+q.element());
		//3. remove
		
		//4. offer(e)
		
		//5. peek()
		
		//6. poll()
	}

}
